<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemFrame215Model" type="com.oumaymasapplication.app.modules.itemframe215.data.model.DrawerItemFrame215Model"/>
    </data>
    <FrameLayout android:id="@+id/frameStackellipsesixtysix"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageEllipseSixtySix"
            android:layout_gravity="top|end"
            android:layout_height="@dimen/_144pxh"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="@dimen/_143pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_ellipse64"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse64"/>
        <View
            android:background="@drawable/rectangle_bg_teal_300_radius_54_295"
            android:id="@+id/viewEllipseSixtyFive"
            android:layout_gravity="top|end"
            android:layout_height="@dimen/_108pxh"
            android:layout_marginBottom="@dimen/_35pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_35pxv" android:layout_width="@dimen/_108pxh"/>
        <LinearLayout android:id="@+id/linearColumnlogofour"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_3">
            <ImageView android:id="@+id/imageLogoFour"
                android:layout_gravity="start"
                android:layout_height="@dimen/_228pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="@dimen/_240pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_logo3}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_logo3"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{drawerItemFrame215Model.txtLanguage}"
                style="@style/txtPoppinssemibold24" tools:text="@string/msg_sign_in_to_your"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_46pxh"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{drawerItemFrame215Model.txtEmail}"
                style="@style/txtPoppinssemibold12_2" tools:text="@string/lbl_email"/>
            <FrameLayout android:id="@+id/frameStackemailOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="@dimen/_332pxh" style="@style/groupStylewhite_5">
                <ImageView android:id="@+id/imageEmailOne"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_email"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_email"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_46pxh"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{drawerItemFrame215Model.txtPassword}"
                style="@style/txtPoppinssemibold12_2" tools:text="@string/lbl_password"/>
            <FrameLayout android:id="@+id/frameStackhide"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_332pxh" style="@style/groupStylewhite_5">
                <ImageView android:id="@+id/imageHide"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_hide"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_hide"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowforgotthepass"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowremeberme"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.51"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRemeberme"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.67"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drawerItemFrame215Model.txtRemeberme}"
                        style="@style/txtPoppinssemibold13" tools:text="@string/lbl_remeber_me"/>

                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/switchMaterialComponentTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.33" android:layout_width="0dp"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotthepass"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.49"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{drawerItemFrame215Model.txtForgotthepass}"
                    style="@style/txtPoppinssemibold12_3" tools:text="@string/msg_forgot_the_pass"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrcontinuewit"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_69pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{drawerItemFrame215Model.txtOrcontinuewit}"
                style="@style/txtPoppinssemibold12_4" tools:text="@string/msg_or_continue_wit"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfacebook"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFacebook"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_facebook}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                <ImageView android:id="@+id/imageGooglePlus"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_googleplus_50x50}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_googleplus_50x50"/>
                <ImageView android:id="@+id/imageAppleLogo"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_applelogo}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_applelogo"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{drawerItemFrame215Model.txtLanguageOne}"
                style="@style/txtPoppinssemibold12_1" tools:text="@string/msg_already_have_ac"/>
        </LinearLayout>
    </FrameLayout>
</layout>
