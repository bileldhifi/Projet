<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listuserRowModel" type="com.oumaymasapplication.app.modules.frame220.data.model.ListuserRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowforward"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_100cornerRadius">
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_weight="0.85" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUser"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_user_black_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEditProfile"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_52pxh"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:letterSpacing="0.01"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{listuserRowModel.txtEditProfile}"
                style="@style/txtRobotomedium14" tools:text="@string/lbl_edit_profile"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageForward"
            android:layout_height="@dimen/_24pxh"
            android:layout_marginBottom="@dimen/_11pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_129pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="@dimen/_24pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_forward"
            tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
    </LinearLayout>
</layout>
