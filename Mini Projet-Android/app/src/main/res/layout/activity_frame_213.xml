<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frame213VM" type="com.oumaymasapplication.app.modules.frame213.data.viewmodel.Frame213VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipsefortysix"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearDeviceDevicef"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <FrameLayout android:id="@+id/frameStackellipsefortysix"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_280pxv"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_86pxv" android:layout_width="@dimen/_342pxh">
                    <View
                        android:background="@drawable/rectangle_bg_teal_400_75_radius_171"
                        android:id="@+id/viewEllipseFortySix"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_57pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_342pxh"/>
                    <ImageView android:id="@+id/imageChatwithdonat"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_279pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_78pxh"
                        android:layout_marginStart="@dimen/_78pxh"
                        android:layout_width="@dimen/_170pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_chatwithdonat}"
